{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "public/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "jsx-no-lambda":false,
    "no-console": false,
    "prefer-const": false,
    "no-debugger": false,
    "ordered-imports": false
  }
}
